.lookbook-popup { position: absolute; bottom: 20px; right: 20px; text-align: right; }
.lookbook-popup > .button {
  --color-button: 255, 255, 255;
  --alpha-button-background: 1;
  --color-button-text: 0, 0, 0;
  --buttons-radius-outset: 100px;
  --buttons-radius: var(--buttons-radius-outset); font-weight: 400; font-size: 14px; line-height: 100%; letter-spacing: 0; padding: 18px 20px; align-items: center; }
.lookbook-popup>.button svg { margin-left: 10px; }
.lookbook-popup>.button:not([disabled]):hover { background: #F5F4F2; }
.lookbook-popup>.button:not([disabled]):hover:after { --color-button: 245, 244, 242; }
.lookbook-popup>.button:not([disabled]).active { background: rgb(var(--color-button-text)); color: rgb(var(--color-button)); }
.lookbook-popup>.button:not([disabled]).active:after { --color-button: 0, 0, 0; }

.lookbook-popup-gird { border-radius: 10px; background: #fff; padding: 20px; min-width: 390px; margin-bottom: 20px; width: 100%; max-width: 390px; text-align: left; box-shadow: 2px 2px 15px rgba(0, 0, 0, 0.1); }
.lookbook-popup:not(.active) .lookbook-popup-gird { height: 0; overflow: hidden; padding-top: 0; padding-bottom: 0; }
.lookbook-popup-gird { margin-bottom: 0; transition: all 0.5s ease; opacity: 0; visibility: hidden; max-height: 487px; overflow: auto; overscroll-behavior: contain; position: absolute; bottom: 100%; right: 0; }
.active .lookbook-popup-gird { margin-bottom: 20px; transition: visibility 0.2s, all 0.3s ease 0.2s; opacity: 1; visibility: visible; }
.lookbook-popup.active:after { content: ''; position: absolute; bottom: 0; right: 0; width: 200%; height: 200%; }
.lookbook-popup-items + .lookbook-popup-items { margin-top: 2px; }
.lobo-popup-card { display: flex; position: relative; }
.lobo-pro-image { max-width: 110px; flex: 1 0 auto; }
.lobo-pro-content { padding-left: 15px; flex: 1 0 auto; }
.lobo-pro-content h3 { font-size: 14px; margin: 0; line-height: 1; letter-spacing: 0; }
.lobo-pro-content .price { margin-top: 8px; font-size: 14px; line-height: 1; letter-spacing: 0; }
.lobo-pro-label { font-size: 12px; line-height: 1; color: #73716E; margin-top: 15px; }

.lobo-pro-content .link { margin-bottom: 0; margin-top: 30px; font-weight: 500; letter-spacing: 0.05em; text-transform: uppercase; font-size: 12px; line-height: 1; }
.lobo-pro-content .link:after { content: ''; position: absolute; top: 0; left: 0; width: 100%; height: 100%; }
.lobo-pro-content .link .icon { width: 16px; margin-left: 8px; }
.lobo-pro-content .link .icon svg { width: 100%; height: auto; }
.lobo-pro-content .link .icon path { stroke-width: 1px; }

.content-overflowed .lookbook-popup-gird { position: relative; left: 40px; z-index: 1; }

@media only screen and (max-width: 989px){
  .lookbook-popup>.button { font-size: 10px; padding: 11px 12px; min-height: 1px; }
  .lookbook-popup>.button svg { width: 12px; height: 12px; margin-left: 8px; }
}
@media only screen and (max-width: 749px){
  .lookbook-popup-gird { padding: 10px; min-width: 290px; max-width: 290px; max-height: 386px; }
  .lobo-pro-image { max-width: 90px; }
