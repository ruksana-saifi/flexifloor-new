  if(localStorage.getItem("lookbook-url")){
    var page_link = localStorage.getItem("lookbook-url");
  }else{
    var page_link = 'https://www.thesareeroom.com/pages/about-us-1?view=lookbook';  
  }
  

  fetch(page_link,{ 
      method:'GET'
    }).then((response) =>{ 
        return response.text();
    }).then(data =>{
        var Products_obj = []; 
        var parser = new DOMParser();
        var doc = parser.parseFromString(data, "text/html");
        var generate_html = '';
        var store_single_html = '';
        var count_prod = doc.querySelectorAll('[data-look-id="'+curr_prod_id+'"]').length;

        if(count_prod > 0){
          var link = document.createElement( "link" );
          link.href = "{{ 'lookbook.css' | asset_url }}";
          link.rel = "stylesheet";
          link.media = "all";
          document.getElementsByTagName("head")[0].appendChild(link);
          var store_id = '';
          count_prod = 0;
          var htmlArray = [];
          doc.querySelectorAll('[data-look-id="'+curr_prod_id+'"]').forEach((divs,index) => {
           if(index == 0){
              store_single_html = divs;
              htmlArray.push(divs.closest('.lookbook-section-one-image').querySelector('.grid__item.lookbook-image.active-img').outerHTML);
              count_prod++;
            }else{
              if(store_id != divs.closest('.shopify-section.lookbook-section').getAttribute('id')){
                //generate_html += `<div class="swiper-slide">` + divs.closest('.lookbook-section-one-image').querySelector('.grid__item.lookbook-image.active-img').outerHTML + `</div>`;
                htmlArray.push(divs.closest('.lookbook-section-one-image').querySelector('.grid__item.lookbook-image.active-img').outerHTML);
                count_prod++;
              }
            }
            store_id = divs.closest('.shopify-section.lookbook-section').getAttribute('id');
          });

          console.log('Total Product to show :',count_prod,htmlArray);

          if(count_prod > 2){
            for(var n=0;n<htmlArray.length;n++){
              if(n==0){
                generate_html = `<div class="swiper-slide">` + htmlArray[n] + `</div>`;
              }else{
                generate_html += `<div class="swiper-slide">` + htmlArray[n] + `</div>`;
              }
            }
            document.querySelector('.product-lookbook .slider-page-width .product-carousel-slider.slide-section .swiper-wrapper').innerHTML = generate_html;
            document.querySelector('.shopify-product-lookbook').classList.remove('hidden');
            if(window.innerWidth > 989){
              proFeaturedLookBookCarousel();  
            }
          }else if(count_prod > 1){
            for(var n=0;n<htmlArray.length;n++){
              if(n==0){
                generate_html = htmlArray[n];
              }else{
                generate_html += htmlArray[n];
              }
            }
            document.querySelector('.product-lookbook .slider-page-width .product-carousel-slider.no-slider .swiper-wrapper').innerHTML = generate_html;
            document.querySelector('.shopify-product-lookbook').classList.remove('hidden');
          }else{
            if(store_single_html.closest('.lookbook-section-one-image').querySelector('.grid__item.lookbook-image.active-img .full-width-img') != null){
               var img_src = store_single_html.closest('.lookbook-section-one-image').querySelector('.grid__item.lookbook-image.active-img .full-width-img').getAttribute('data-src');
               document.querySelector('.product-lookbook .slider-page-width .product-carousel-slider.only-content.full-width .swiper-wrapper').innerHTML = store_single_html.closest('.lookbook-section-one-image').querySelector('.grid__item.lookbook-image.active-img').outerHTML;
               document.querySelector('.product-lookbook .slider-page-width .product-carousel-slider.only-content.full-width .swiper-wrapper .grid__item.active-img > img').setAttribute('src',img_src)
               document.querySelector('.product-lookbook .slider-page-width .product-carousel-slider.only-content.full-width .swiper-wrapper .grid__item.active-img > img').setAttribute('srcset',img_src)
               document.querySelector('.shopify-product-lookbook').classList.remove('hidden');
            }else{
              document.querySelector('.product-lookbook .slider-page-width .product-carousel-slider.only-content.no-fullwd-img .swiper-wrapper').innerHTML = store_single_html.closest('.lookbook-section-one-image').querySelector('.grid__item.lookbook-image.active-img').outerHTML;
              document.querySelector('.shopify-product-lookbook').classList.remove('hidden');
            }
          }
        }else{
          console.log('Total Product to show :',0);
        }
        
        

        
        
        //console.log(generate_html,'Products_obj');
    });

});
